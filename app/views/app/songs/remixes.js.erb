$("#mixes_<%= @song.id %>").html("<%= escape_javascript(render partial: "remixes", locals: {song: @song} ) %>");
$.each($('#mixes_<%= @song.id %> .mix8-player'), function(index, player) {
	initializePlayer('#' + $(player).find('.jp-player').prop('id'), '#' + $(player).find('.jp-audio').prop('id'));
});
