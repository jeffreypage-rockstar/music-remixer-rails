window.configuration = "<%=@configuration%>";
$("#mixaudioplayer").html("<%=j render 'mixaudioplayer', song: @song, mixaudio: @mixaudio %>");
<% if @mixaudio.present? %>
	stopTopPlayer();
    initializeTopPlayer("<%=@mixaudio%>");
<% end %>
$(window.configurationBtn).button('reset');
setTimeout(function(){
	$(window.configurationBtn).addClass("disabled");
}, 500);